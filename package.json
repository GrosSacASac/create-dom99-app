{
  "name": "create-dom99-app",
  "version": "173.0.0",
  "description": "starter template for static website development",
  "scripts": {
    "inline-html": "node tools/inlineHTML.js source/home.html",
    "bundle-js": "parcel build source/final-home.html",
    "build-prod": "npm run inline-html && npm run bundle-js",
    "serve-prod": "serve ./dist -p 8080",
    "serve-dev": "node auto-reload-server/auto-reload-server.js"
  },
  "devDependencies": {
    "chokidar": "^3.0.1",
    "express": "^4.17.1",
    "minimist": "^1.2.0",
    "parcel-bundler": "^1.12.3",
    "serve": "^11.0.1",
    "serve-static": "^1.14.1",
    "utilsac": "^9.7.2",
    "ws": "^7.0.0"
  },
  "dependencies": {
    "dom99": "^19.1.1"
  }
}
