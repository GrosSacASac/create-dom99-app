{
  "name": "create-dom99-app",
  "version": "173.0.2",
  "description": "starter template for static website development",
  "type": "module",
  "scripts": {
    "inline-html": "node --experimental-modules tools/inlineHTML.js source/home.html",
    "bundle-js": "parcel build source/final-home.html",
    "build-prod": "npm run inline-html && npm run bundle-js",
    "serve-prod": "serve ./dist -p 8080",
    "serve-dev": "node --experimental-modules auto-reload-server/auto-reload-server.js"
  },
  "devDependencies": {
    "chokidar": "^3.0.1",
    "filesac": "^11.0.0",
    "minimist": "^1.2.0",
    "onewaydata": "^4.0.0",
    "parcel-bundler": "^1.12.3",
    "polka": "^0.5.2",
    "serve": "^11.0.1",
    "serve-static": "^1.14.1"
  },
  "dependencies": {
    "dom99": "^20.0.2"
  }
}
